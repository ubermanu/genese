{
  "banner": false,
  "compression": "GZ",
  "output": "dist/genese.phar",
  "directories": [
    "_templates",
    "bin",
    "src"
  ],
  "finder": [
    {
      "notName": "/LICENSE|.*\\.md|.*\\.dist|Makefile|composer\\.json|composer\\.lock/",
      "exclude": [
        "doc",
        "test",
        "test_old",
        "tests",
        "Tests",
        "vendor-bin"
      ],
      "in": "vendor"
    }
  ],
  "compactors": [
    "KevinGH\\Box\\Compactor\\Php",
    "KevinGH\\Box\\Compactor\\Json"
  ]
}
