{
    "name": "ubermanu/genese",
    "description": "Code generator based on templates",
    "license": "MIT",
    "version": "1.7.0",
    "authors": [
        {
            "name": "ubermanu",
            "email": "manu@paca.happy-dev.fr"
        }
    ],
    "require": {
        "webuni/front-matter": "^1.3",
        "twig/twig": "^3.0",
        "symfony/console": "^5.3",
        "symfony/dotenv": "^5.3",
        "symfony/yaml": "^5.3"
    },
    "suggest": {
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Genese\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Genese\\Tests\\": "tests/"
        }
    },
    "bin": [
        "bin/genese"
    ]
}
